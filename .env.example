# Domain Configuration
TRAEFIK_DASHBOARD_DOMAIN=traefik.yourdomain.com

# Security
ADMIN_AUTH=admin:$2y$05$hashedpasswordhere

# Rate Limiting
RATE_LIMIT_AVERAGE=100
RATE_LIMIT_BURST=50

# TLS Configuration
TLS_MIN_VERSION=VersionTLS13
CERT_FILE_PATH=/etc/certs/cert.pem
KEY_FILE_PATH=/etc/certs/key.pem

# SSL/TLS Parameters
STS_SECONDS=31536000

# Let's Encrypt Configuration
ACME_EMAIL=your-email@domain.com

# Fail2Ban Configuration
FAIL2BAN_MAXRETRY=5
FAIL2BAN_FINDTIME=600
FAIL2BAN_BANTIME=3600
